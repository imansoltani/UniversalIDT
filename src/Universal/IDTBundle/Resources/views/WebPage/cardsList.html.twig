{% for product in products %}
    <div class="item"><!-- SINGLE CALLING CARD -->
        <div class="single-card clearfix">
            <a href="javascript:openDetails('{{ product.id }}', true)" title="{{ product.name }}" data-id="{{ product.id }}">
                <img src="{{ asset(product.webPath) }}" alt="{{ product.id }}">
            </a>
        </div>
    </div>
{% endfor %}